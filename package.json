{
  "name": "webpack4-demo",
  "version": "1.0.0",
  "description": "Exploring all of Webpack 4's features",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --env development\"",
    "build": "webpack --env production --json > stats.json",
    "build:ssr": "webpack --config webpack.ssr.js",
    "ssr": "node_modules/.bin/browser-refresh ./server.js",
    "build:i18n": "webpack --config webpack.i18n.js",
    "test:mocha": "mocha tests",
    "test:mocha:watch": "webpack-dev-server --hot --config webpack.mocha.js",
    "test:karma": "karma start",
    "test:karma:watch": "Karma start --auto-watch --no-single-run",
    "test:jest:watch": "jest --watch",
    "test:jest": "jest",
    "deploy": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmoDinho/webpack4-demo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AmoDinho/webpack4-demo/issues"
  },
  "homepage": "https://github.com/AmoDinho/webpack4-demo#readme",
  "devDependencies": {
    "autoprefixer": "^8.6.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "browser-refresh": "^1.7.3",
    "clean-webpack-plugin": "^0.1.19",
    "cssnano": "^4.0.2",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "gh-pages": "^1.2.0",
    "git-revision-webpack-plugin": "^3.0.3",
    "glob": "^7.1.2",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "i18n-webpack-plugin": "^1.0.0",
    "jest": "^23.4.1",
    "karma": "^2.0.4",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^3.0.0",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^5.2.0",
    "mocha-loader": "^1.1.3",
